generator client {
  provider        = "prisma-client-js"
  output          = "../node_modules/.prisma/client"
  previewFeatures = ["fullTextSearch"]
}

datasource db {
  provider = "postgres"
  url      = env("TEST_POSTGRES_URI")
}

model User {
  id        String @id @default(cuid()) @db.VarChar(30)
  firstName String
  lastName  String
}
