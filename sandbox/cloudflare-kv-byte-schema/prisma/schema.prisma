generator client {
  provider        = "prisma-client-js"
  output          = "./client"
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(uuid())
  email     String   @unique
  age       Int
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
